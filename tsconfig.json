{
    "compilerOptions": {
      "module": "commonjs",
      "noImplicitAny": false,
      "noUnusedLocals": false,
      "removeComments": true,
      "noLib": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "useUnknownInCatchVariables": false,
      "target": "ES2021",
      "sourceMap": true,
      "allowJs": false,
      "outDir": "dist",
    },
    "exclude": ["node_modules"]
  }